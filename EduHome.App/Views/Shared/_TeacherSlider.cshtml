@model IEnumerable<Teacher>
@foreach (var item in Model)
{
    <div class="testimonial-info">
        <div style="height:74px;" class="testimonial-img">
            <img style="width:100%;height:100%;" src="~/img/teacher/@item.Image" alt="testimonial">
        </div>
        <div class="testimonial-content">
            <p>@item.AboutMe</p>
            <h4>@item.FullName</h4>
            <h5>@item.Position?.Name, @item.Degree?.Name</h5>
        </div>
    </div>
}